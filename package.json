{
  "name": "ngx-geo-api-gouv",
  "version": "21.0.0",
  "author": "PlaceMe SAS",
  "license": "MIT",
  "homepage": "https://github.com/PlaceMe-SAS/ngx-geo-api-gouv/README.md",
  "repository": {
    "type": "git",
    "url": "git@github.com:PlaceMe-SAS/ngx-geo-api-gouv.git"
  },
  "bugs": {
    "url": "https://github.com/PlaceMe-SAS/ngx-geo-api-gouv/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ngx-geo-api-gouv-address",
    "build:dev": "npm run build --configuration development && cd dist/ngx-geo-api-gouv-address && npm pack",
    "test": "ng test --watch=false",
    "lint": "npm-run-all lint:*",
    "lint:ng": "ng lint",
    "lint:prettier": "prettier --list-different \"**/*.*\"",
    "fix": "npm-run-all fix:*",
    "fix:ng": "npm run lint:ng -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "publish": "npm run build && npm login && npm publish ./dist/ngx-geo-api-gouv-address"
  },
  "private": true,
  "prettier": {
    "bracketSpacing": true,
    "endOfLine": "lf",
    "printWidth": 140,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "packageManager": "npm@11.6.3",
  "dependencies": {
    "@angular/animations": "^21.0.8",
    "@angular/common": "^21.0.8",
    "@angular/compiler": "^21.0.8",
    "@angular/core": "^21.0.8",
    "@angular/platform-browser": "^21.0.8",
    "@angular/platform-browser-dynamic": "^21.0.8",
    "geojson": "^0.5.0",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^21.1.0",
    "@angular-eslint/eslint-plugin": "^21.1.0",
    "@angular-eslint/eslint-plugin-template": "^21.1.0",
    "@angular-eslint/schematics": "^21.1.0",
    "@angular-eslint/template-parser": "^21.1.0",
    "@angular/build": "^21.0.5",
    "@angular/cli": "^21.1.4",
    "@angular/compiler-cli": "^21.0.8",
    "@types/geojson": "^7946.0.16",
    "angular-eslint": "^21.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jsdom": "^27.4.0",
    "ng-packagr": "^21.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.52.0",
    "vitest": "^4.0.16"
  }
}
